	[10390] = {bid=10390, name="神装祈祷对话", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[神装是由矮人族使用神石锻造出来的特别装备，可以为英雄额外附加特殊属性和技能！]]},{'unit_dialog',0,5,1,235,1,[[小助手]],[[通过天界祈祷和天界副本可以获得神装，现在就一起去看看吧~]]}}, desc=""},
	[10391] = {bid=10391, name="神装祈祷引导", is_skip=0, is_switch=0, act={{'play_guide',10390}}, desc=""},
