	[10410] = {bid=10410, name="神装副本对话", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[神装凑齐两件套和四件套时还能拥有更强大的效果！天界副本中也会掉落神装和材料哟~]]}}, desc=""},
