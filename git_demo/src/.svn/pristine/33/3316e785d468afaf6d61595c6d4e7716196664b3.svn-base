	[10420] = {bid=10420, name="原力水晶对话", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[哇，发现了神秘的原力水晶！我们的英雄可以加入水晶的槽位之中，从而提升为和水晶相同的等级！]]},{'unit_dialog',0,5,1,235,1,[[小助手]],[[而水晶的等级，则取决于我们最高级的5个英雄！]]}}, desc=""},
