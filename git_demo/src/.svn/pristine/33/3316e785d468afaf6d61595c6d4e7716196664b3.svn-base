	[10360] = {bid=10360, name="新手训练营剧情", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[了解这片大陆的规则有助于你更好地应对危机！]]},{'unit_dialog',0,5,1,235,1,[[小助手]],[[现在就让我们一起去新手训练营学习一下吧~]]}}, desc=""},
