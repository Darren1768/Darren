	[10372] = {bid=10372, name="", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[接下来就不断地向前探索吧！位面之境每<div fontcolor=#d95014>2天的0点</div>会自动重置，探索得越多奖励越丰厚哦~]]}}, desc=""},
	[10380] = {bid=10380, name="精灵剧情", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[精灵可以依附于神器之上进行战斗，在固定回合释放强力技能！]]},{'unit_dialog',0,5,1,235,1,[[小助手]],[[而精灵可以通过孵化精灵蛋获得，现在就一起去看看吧~]]}}, desc=""},
	[10381] = {bid=10381, name="精灵引导", is_skip=0, is_switch=0, act={{'play_guide',10380}}, desc=""},
	[10382] = {bid=10382, name="精灵孵化说明", is_skip=0, is_switch=0, act={{'unit_dialog',0,5,1,235,1,[[小助手]],[[等待孵化倒计时结束后就可以获取精灵了，运气好的话还有可能获得极其稀有的橙色精灵~]]},{'unit_dialog',0,5,1,235,1,[[小助手]],[[当然，还可以直接召唤精灵！通过许愿池许愿后再召唤，有更高概率获得指定精灵哦~]]}}, desc=""},
